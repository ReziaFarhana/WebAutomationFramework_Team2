<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="BBCTestSuit">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserStack"/>
    <parameter name="OS" value="windows"/>
    <parameter name="os_version" value="10"/>
    <!--<parameter name="OS" value="OS X"/>-->
    <!--<parameter name="os_version" value="Sierra"/>-->
    <!--  <parameter name="browserName" value="chrome"/>-->
    <!--  <parameter name="browserVersion" value="89"/>-->
          <parameter name="browserName" value="firefox"/>
          <parameter name="browserVersion" value="76.0"/>


    <parameter name="url" value="https://www.bbc.com/"/>


    <test name="BBC Automation Test Script using chrome">

        <classes>

            <class name="bbcTest.AboutUsTest"></class>
            <class name="bbcTest.DestinationTest"></class>
            <class name="bbcTest.DiscoveryTest"></class>
            <class name="bbcTest.FoodAndCultureTest"></class>
        </classes>

    </test>
</suite>





