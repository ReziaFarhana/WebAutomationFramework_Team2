<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="GeicoTestSuit" parallel="none" thread-count="1">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserStack"/>
    <parameter name="OS" value="windows"/>
    <parameter name="os_version" value="10"/>
    <!--    <parameter name="OS" value="OS X"/>-->
    <!--    <parameter name="os_version" value="Sierra"/>-->
    <!--    <parameter name="browserName" value="chrome"/>-->
    <!--    <parameter name="browserVersion" value="89"/>-->
    <!--    <parameter name="browserName" value="firefox"/>-->
    <!--    <parameter name="browserVersion" value="76.0"/>-->
    <parameter name="url" value="https://www.geico.com/"/>


    <test name="Geico Automation Test Script using chrome">
        <parameter name="browserName" value="firefox"/>
        <parameter name="browserVersion" value="86"/>
        <classes>
            <class name="test_page.GeicoTest">
                <methods>

                    <exclude name="VerifyGeicoHomeIsLoaded"/>
                    <exclude name="verifyBusinessOwner"/>
                    <exclude name="verifyGeneralLiabilityIns"/>
                    <exclude name="verifyProfessionalLiabilityLink"/>
                    <exclude name="verifyWorkersConpensatinInsLink"/>
                    <exclude name="verifyCyberLiabilityInsLink"/>
                    <exclude name="verifyGetQuoteGeneral"/>
                    <exclude name="verifyStartYourQuoteNowInGeneralIns"/>
                    <exclude name="verifyAutoInsuranceInVehicle"/>
                    <exclude name="verifyQuoteWithZIP"/>
                    <exclude name="verifyOnNextLink"/>
                    <exclude name="verifyUpdateDateOfBirth"/>
                    <exclude name="verifyUpdateNameInformation"/>
                    <exclude name="verifyMotorcycleInsuranceSelected"/>
                    <exclude name="verifyMotorInsurance"/>
                    <exclude name="verifyNoForAutoInsurance"/>
                    <exclude name="verifyPropertyInsurance"/>
                    <exclude name="verifyHowToProtectMyHome"/>
                    <exclude name="verifyOnHowMuchCoverage"/>
                    <exclude name="verifyContinueQuoteButton"/>


                </methods>

            </class>

        </classes>

    </test>

</suite>





